{
    "linters": {
        "disable-all": true,
        "enable": [
            "govet",
            "goimports",
            "misspell",
            "ineffassign",
            "gofmt"
        ]
    },
    "linters-settings": {
        "revive": {
            "exclude-use": [
                "dot-imports"
            ]
        }
    },
    "run": {
        "issues": {
            "exclude-files": [
                "/zz_generated_",
                "*.pb.go"
            ]
        },
        "deadline": "5m"
    }
}