{
    "linters": {
        "disable-all": true,
        "enable": [
            "govet",
            "goimports",
            "misspell",
            "ineffassign",
            "gofmt"
        ]
    },
    "linters-settings": {
        "revive": {
            "exclude-use": [
                "dot-imports"
            ]
        }
    },
    "run": {
        "skip-files": [
            "/zz_generated_"
        ],
        "deadline": "5m"
    }
}